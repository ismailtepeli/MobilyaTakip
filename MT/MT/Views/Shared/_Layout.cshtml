<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>@ViewBag.Title - Mobilya Takip</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,700,900" rel="stylesheet">
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
 
    <!-- Bootstrap CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/css/font-awesome.min.css">
    <!-- owl.carousel CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/css/owl.carousel.css">
    <link rel="stylesheet" href="~/Content/css/owl.theme.css">
    <link rel="stylesheet" href="~/Content/css/owl.transitions.css">
    <!-- meanmenu CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/css/meanmenu/meanmenu.min.css">
    <!-- animate CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/css/animate.css">
    <!-- normalize CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/css/normalize.css">
    <!-- mCustomScrollbar CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/css/scrollbar/jquery.mCustomScrollbar.min.css">
    <!-- jvectormap CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/css/jvectormap/jquery-jvectormap-2.0.3.css">
    <!-- notika icon CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/css/notika-custom-icon.css">
    <!-- wave CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/css/wave/waves.min.css">
    <!-- main CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/css/main.css">
    <!-- style CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/css/datapicker/datepicker3.css">
    <link href="~/Content/css/style.css" rel="stylesheet" />
    <!-- responsive CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/css/responsive.css">
    <!-- modernizr JS
        ============================================ -->
    <!-- End Footer area-->
    <!-- jquery
        ============================================ -->
    <script src="~/Scripts/js/vendor/jquery-1.12.4.min.js"></script>
    <!-- bootstrap JS
        ============================================ -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay" crossorigin="anonymous">
 
    <script src="~/Scripts/js/bootstrap.min.js"></script>
    <script src="~/Scripts/js/vendor/modernizr-2.8.3.min.js"></script> 

    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css" />

    <script type="text/javascript" src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>

    <link href="~/Content/css/bootstrap-select/bootstrap-select.css" rel="stylesheet" />
    

 
    <script src="~/Scripts/js/datapicker/bootstrap-datepicker.js"></script>
    <script src="~/Scripts/js/datapicker/datepicker-active.js"></script>
    <script src="~/Scripts/MyLib/MtsLib.js"></script>

</head>
<body>
    <div class="header-top-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                    <div class="logo-area">
                        <a href="#"><img src="~/img/logo/logo.png" alt="" /></a>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <!-- End Header Top Area -->
    <!-- Mobile Menu start -->
    <div class="mobile-menu-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="mobile-menu">
                        <nav id="dropdown">
                            <ul class="mobile-menu-nav">
                                <li>
                                    <a  href="/Home/Index">Anasayfa</a>

                                </li>
                                <li>
                                    <a data-toggle="collapse" data-target="#demoevent" href="#">Kullanıcı</a>
                                    <ul id="demoevent" class="collapse dropdown-header-top">
                                        <li><a href="/User/Index">Kullanıcı Listesi</a></li>
                                        <li><a href="/User/AddUsers">Kullanıcı Ekle</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a data-toggle="collapse" data-target="#democrou" href="#">Sipariş</a>
                                    <ul id="democrou" class="collapse dropdown-header-top">
                                        <li><a href="/Order/AddOrder">Sipariş Ekle</a></li>
                                        <li><a href="/Order/Index">Sipariş Listesi</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a data-toggle="collapse" data-target="#demolibra" href="#">Ürün</a>
                                    <ul id="demolibra" class="collapse dropdown-header-top">
                                        <li><a href="/Product/ProductAdd">Ürün Ekle</a></li>
                                        <li><a href="/Product/Index">Ürün Listesi</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a data-toggle="collapse" data-target="#demodepart" href="#">Model</a>
                                    <ul id="demodepart" class="collapse dropdown-header-top">
                                        <li><a href="/MDL/Index">Model Listesi</a></li>
                                        <li><a href="/MDL/AddMdl">Model Ekle</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a data-toggle="collapse" data-target="#Kumas" href="#">Kartela</a>
                                    <ul id="demodepart" class="collapse dropdown-header-top">
                                        <li><a href="/Catalog/Index">Kartela Listesi</a></li>
                                        <li><a href="/Catalog/AddCatalog">Kartela Ekle</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a data-toggle="collapse" data-target="#Kp" href="#">Kartela Ürünü</a>
                                    <ul id="demodepart" class="collapse dropdown-header-top">
                                        <li><a href="/CatalogProduct/Index">Kartela Ürün Listesi</a></li>
                                        <li><a href="/CatalogProduct/CatalogProductAdd">Kartelaya Ürün Ekle</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Mobile Menu end -->
    <!-- Main Menu area start-->
    <div class="main-menu-area mg-tb-40">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <ul class="nav nav-tabs notika-menu-wrap menu-it-icon-pro">
                        <li >
                            <a href="/Home/Index"><i class="notika-icon notika-house"></i> Anasayfa</a>
                        </li>
                        <li>
                            <a data-toggle="tab" href="#mailbox"><i class="notika-icon notika-mail"></i> Kullanıcı </a>
                        </li>
                        <li>
                            <a data-toggle="tab" href="#Interface"><i class="notika-icon notika-edit"></i> Sipariş </a>
                        </li>
                        <li>
                            <a data-toggle="tab" href="#Charts"><i class="notika-icon notika-bar-chart"></i> Ürün</a>
                        </li>
                        <li>
                            <a data-toggle="tab" href="#Tables"><i class="notika-icon notika-windows"></i> Model</a>
                        </li>
                        <li>
                            <a data-toggle="tab" href="#Kumas"><i class="notika-icon notika-windows"></i> Kartela</a>
                        </li>
                        
                    </ul>
                    <div class="tab-content custom-menu-content">
                        <div id="Home" class="tab-pane in active notika-tab-menu-bg animated flipInX">

                        </div>
                        <div id="mailbox" class="tab-pane notika-tab-menu-bg animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                <li>
                                    <a href="/User/Index">Kullanıcı Listesi</a>
                                </li>
                                <li>
                                    <a href="/User/AddUsers">Kullanıcı Ekle</a>
                                </li>

                            </ul>
                        </div>
                        <div id="Interface" class="tab-pane notika-tab-menu-bg animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                <li>
                                    <a href="/Order/Index">Sipariş Listesi</a>
                                </li>
                                <li>
                                    <a href="/Order/AddOrder">Sipariş Ekle</a>
                                </li>

                            </ul>
                        </div>
                        <div id="Charts" class="tab-pane notika-tab-menu-bg animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                <li>
                                    <a href="/Product/Index">Ürün Listesi</a>
                                </li>
                                <li>
                                    <a href="/Product/ProductAdd">Ürün Ekle</a>
                                </li>

                            </ul>
                        </div>
                        <div id="Tables" class="tab-pane notika-tab-menu-bg animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                <li>
                                    <a href="/MDL/Index">Model Listesi</a>
                                </li>
                                <li>
                                    <a href="/MDL/AddMdl">Model Ekle</a>
                                </li>
                            </ul>
                        </div>
                        <div id="Kumas" class="tab-pane notika-tab-menu-bg animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                <li>
                                    <a href="/Catalog/AddCatalog">Kartela Ekle</a>
                                </li>
                                <li>
                                    <a href="/Catalog/Index">Kartela Listesi</a>
                                </li>
                                <li>
                                    <a href="/CatalogProduct/Index">Ürün Listesi</a>
                                </li>
                                <li>
                                    <a href="/CatalogProduct/CatalogProductAdd">Ürün Ekle</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="notika-status-area">
        @RenderBody()
       
    </div>


    <!-- wow JS
        ============================================ -->
    <script src="~/Scripts/js/wow.min.js"></script>
    <!-- price-slider JS
        ============================================ -->
    <script src="~/Scripts/js/jquery-price-slider.js"></script>
    <!-- owl.carousel JS
        ============================================ -->
    <script src="~/Scripts/js/owl.carousel.min.js"></script>
    <!-- scrollUp JS
        ============================================ -->
    <script src="~/Scripts/js/jquery.scrollUp.min.js"></script>
    <!-- meanmenu JS
        ============================================ -->
    <script src="~/Scripts/js/meanmenu/jquery.meanmenu.js"></script>
    <!-- counterup JS
        ============================================ -->
    <script src="~/Scripts/js/counterup/jquery.counterup.min.js"></script>
    <script src="~/Scripts/js/counterup/waypoints.min.js"></script>
    <script src="~/Scripts/js/counterup/counterup-active.js"></script>
    <!-- mCustomScrollbar JS
        ============================================ -->
    <script src="~/Scripts/js/scrollbar/jquery.mCustomScrollbar.concat.min.js"></script>
    <!-- jvectormap JS
        ============================================ -->
    <script src="~/Scripts/js/jvectormap/jquery-jvectormap-2.0.2.min.js"></script>
    <script src="~/Scripts/js/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/Scripts/js/jvectormap/jvectormap-active.js"></script>
    <!-- sparkline JS
        ============================================ -->
    <script src="~/Scripts/js/sparkline/jquery.sparkline.min.js"></script>
    <script src="~/Scripts/js/sparkline/sparkline-active.js"></script>
    <!-- sparkline JS
        ============================================ -->
    <script src="~/Scripts/js/flot/jquery.flot.js"></script>
    <script src="~/Scripts/js/flot/jquery.flot.resize.js"></script>
    <script src="~/Scripts/js/flot/curvedLines.js"></script>
    <script src="~/Scripts/js/flot/flot-active.js"></script>
    <!-- knob JS
        ============================================ -->
    <script src="~/Scripts/js/knob/jquery.knob.js"></script>
    <script src="~/Scripts/js/knob/jquery.appear.js"></script>
    <script src="~/Scripts/js/knob/knob-active.js"></script>
    <!--  wave JS
        ============================================ -->
    <script src="~/Scripts/js/wave/waves.min.js"></script>
    <script src="~/Scripts/js/wave/wave-active.js"></script>
    <!--  todo JS
        ============================================ -->
    <script src="~/Scripts/js/todo/jquery.todo.js"></script>
    <script src="~/Scripts/js/bootstrap-select/bootstrap-select.js"></script>
    <!-- plugins JS
        ============================================ -->
    <script src="~/Scripts/js/plugins.js"></script>
    <!--  Chat JS
        ============================================ -->
    <script src="~/Scripts/js/chat/moment.min.js"></script>
    <script src="~/Scripts/js/chat/jquery.chat.js"></script>
    <!-- main JS
        ============================================ -->
    <script src="~/Scripts/js/main.js"></script>
</body>
</html>
